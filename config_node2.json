{
    "server": {
        "port": ":4224",
        "data_dir": "data_node2"
    },
    "storage": {
        "max_segment_size": 10485760
    },
    "retention": {
        "max_age": "1h",
        "max_bytes": 1073741824,
        "check_interval": "5s"
    },
    "ack": {
        "timeout": "5s",
        "max_retries": 1
    },
    "durable": {
        "flush_interval": "1s",
        "replay_batch": 100
    },
    "dlq": {
        "prefix": "DLQ."
    },
    "flow_control": {
        "enable_rate_limit": false,
        "backpressure_mode": "drop"
    },
    "deduplication": {
        "enabled": true,
        "window_size": "5m",
        "max_entries": 10000
    },
    "cluster": {
        "enabled": true,
        "node_id": "node-2",
        "bind_addr": "127.0.0.1:5224",
        "advertise_addr": "127.0.0.1:5224",
        "data_dir": "data_node2/raft",
        "bootstrap": false,
        "election_timeout": "3s",
        "heartbeat_interval": "1s"
    }
}